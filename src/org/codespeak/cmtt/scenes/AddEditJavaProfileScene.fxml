<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="201.0" prefWidth="502.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.codespeak.cmtt.scenes.AddEditJavaProfileSceneController">
   <children>
      <Label fx:id="headerLabel" alignment="TOP_CENTER" layoutX="14.0" layoutY="14.0" prefHeight="35.0" prefWidth="476.0" text="Add Java Profile">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label layoutX="20.0" layoutY="62.0" text="Profile Name">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="profileNameInput" layoutX="120.0" layoutY="60.0" prefHeight="25.0" prefWidth="362.0" />
      <Label layoutX="20.0" layoutY="98.0" text="Path to Java executable">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button layoutX="20.0" layoutY="128.0" mnemonicParsing="false" onAction="#onSelectFileButtonClick" text="Select File ..." />
      <Label fx:id="javaExecutablePathLabel" layoutX="106.0" layoutY="132.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button layoutX="248.0" layoutY="160.0" mnemonicParsing="false" onAction="#onOKButtonClick" prefHeight="25.0" prefWidth="107.0" text="OK" />
      <Button layoutX="375.0" layoutY="160.0" mnemonicParsing="false" onAction="#onCloseButtonClick" prefHeight="25.0" prefWidth="107.0" text="Close" />
   </children>
</AnchorPane>
