<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="208.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.codespeak.cmtt.scenes.SettingsSceneController">
   <children>
      <Label fx:id="settingsLabel" alignment="TOP_CENTER" layoutX="14.0" layoutY="14.0" prefHeight="30.0" prefWidth="565.0" text="Settings for %p">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="64.0" text="Minecraft Launcher Location">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button layoutX="14.0" layoutY="95.0" mnemonicParsing="false" onAction="#onMinecraftLauncherselectFile" text="Select File ..." />
      <Label fx:id="minecraftLauncherLocationLabel" layoutX="102.0" layoutY="99.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button layoutX="394.0" layoutY="166.0" mnemonicParsing="false" onAction="#onOKButtonClick" prefHeight="25.0" prefWidth="78.0" text="OK" />
      <Button layoutX="501.0" layoutY="166.0" mnemonicParsing="false" onAction="#onCloseButtonClick" prefHeight="25.0" prefWidth="78.0" text="Close" />
      <CheckBox fx:id="checkUpdateOnStartupCheck" layoutX="18.0" layoutY="138.0" mnemonicParsing="false" text="Check Update On Startup">
         <font>
            <Font size="14.0" />
         </font>
      </CheckBox>
   </children>
</AnchorPane>
