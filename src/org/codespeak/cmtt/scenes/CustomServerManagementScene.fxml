      <Button layoutX="143.0" layoutY="433.0" mnemonicParsing="false" onAction="#onDeleteServerTypeButtonClick" text="Delete Server Type" />
      <Button layoutX="271.0" layoutY="433.0" mnemonicParsing="false" onAction="#onEditServerTypeButtonClick" prefHeight="25.0" prefWidth="116.0" text="Edit Server Type" />
      <Button fx:id="cancelEditButton" disable="true" layoutX="397.0" layoutY="433.0" mnemonicParsing="false" onAction="#onCancelEditButtonClick" prefHeight="25.0" prefWidth="116.0" text="Cancel Edit" />
      <Button layoutX="397.0" layoutY="493.0" mnemonicParsing="false" onAction="#onCloseWindowButtonClick" prefHeight="25.0" prefWidth="116.0" text="Close Window" />
   </children>
