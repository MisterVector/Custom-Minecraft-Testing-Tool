<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="526.0" prefWidth="534.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.codespeak.cmtt.scenes.CustomServerManagementSceneController">
   <children>
      <Label alignment="TOP_CENTER" layoutX="21.0" layoutY="14.0" prefHeight="35.0" prefWidth="496.0" text="Custom Server Types Management">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label alignment="TOP_LEFT" layoutX="23.0" layoutY="61.0" prefHeight="44.0" prefWidth="496.0" text="If you are testing on a new server platform that has different startup parameters for the worlds folder or plugins folder, you can create a definition for that server here." wrapText="true" />
      <Label layoutX="17.0" layoutY="117.0" text="Server Name">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="serverNameInput" layoutX="143.0" layoutY="114.0" prefHeight="25.0" prefWidth="368.0" />
      <Label layoutX="17.0" layoutY="147.0" text="Plugins Argument">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="pluginsArgumentInput" layoutX="145.0" layoutY="144.0" prefHeight="25.0" prefWidth="368.0" />
      <Label layoutX="17.0" layoutY="177.0" text="Worlds Argument">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="worldsArgumentInput" layoutX="145.0" layoutY="174.0" prefHeight="25.0" prefWidth="368.0" />
