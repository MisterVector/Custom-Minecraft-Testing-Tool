<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="384.0" prefWidth="493.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.codespeak.cmtt.scenes.MainSceneController">
    <children>
      <MenuBar prefHeight="25.0" prefWidth="493.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Quit" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Profile">
              <items>
                  <MenuItem mnemonicParsing="false" onAction="#onJVMFlagsMenuClick" text="JVM Flags ..." />
                  <MenuItem mnemonicParsing="false" onAction="#onServersMenuItemClick" text="Servers ..." />
              </items>
            </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <AnchorPane layoutX="4.0" layoutY="25.0" prefHeight="393.0" prefWidth="486.0">
         <children>
            <Label alignment="TOP_CENTER" layoutX="14.0" layoutY="14.0" prefHeight="35.0" prefWidth="458.0" text="Development Profile Management">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
            <Label alignment="TOP_CENTER" layoutX="14.0" layoutY="56.0" prefHeight="20.0" prefWidth="458.0" text="Manage plugin and server development profiles">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <ListView fx:id="pluginDevelopmentProfileList" layoutX="14.0" layoutY="86.0" prefHeight="216.0" prefWidth="458.0" />
            <Button layoutX="14.0" layoutY="316.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="215.0" text="Open Profile" />
            <Button layoutX="14.0" layoutY="356.0" mnemonicParsing="false" onAction="#onAddPluginDevelopmentProfileButtonClick" prefHeight="25.0" prefWidth="215.0" text="Add Plugin Development Profile" />
            <Button layoutX="257.0" layoutY="356.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="215.0" text="Add Server Development Profile" />
            <Button layoutX="257.0" layoutY="316.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="215.0" text="Delete Profile" />
         </children>
      </AnchorPane>
    </children>
</AnchorPane>
