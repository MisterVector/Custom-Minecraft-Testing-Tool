      <Label layoutX="389.0" layoutY="62.0" text="Custom Plugins Argument">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="customPluginsArgumentInput" disable="true" layoutX="573.0" layoutY="60.0" prefHeight="25.0" prefWidth="176.0" />
      <Label layoutX="389.0" layoutY="93.0" text="Custom Worlds Argument">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="customWorldsArgumentInput" disable="true" layoutX="573.0" layoutY="90.0" prefHeight="25.0" prefWidth="176.0" />
